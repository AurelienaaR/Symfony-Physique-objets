<html>

  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>
      {% block title %}
           Erreur sur le site Eléments de physique en méthodes et langages objet 
      {% endblock %}
    </title>
    {% block stylesheets %}
      {% stylesheets filter='cssrewrite' 'bundles/sitephysphysmvc/css/main.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
      {% endstylesheets %}
      {% stylesheets filter='cssrewrite' 'bundles/sitephysphysmvc/css/bootstrap.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css">
      {% endstylesheets %}
    {% endblock %}
  </head>

  <body>
    {% block menu %}
      <div id="menu_nav">
        <ul class="menu cf">
          <li><a href="{{ path('sitephys_physmvc_home') }}">Accueil</a></li>
          <li><a href="{{ path('sitephys_physmvc_topic') }}">Thèmes</a></li>
          <li><a href="{{ path('sitephys_physmvc_symbolization') }}">Niveaux</a></li>
          <li><a href="{{ path('sitephys_physmvc_edition') }}">Edition</a></li>
          <li><a href="{{ asset('/login') }}"> {% if userconnect is defined %} {{ userconnect }} {% else %} {{ "Connexion" }} {% endif %}</a></li>
          <li><a href="{{ asset('../logout') }}">Déconnexion</a></li>
          <li><a href="{{ path('sitephys_user_account') }}">Compte</a></li>
          <li><a href="http://enseignant_postbac.nexgate.ch/index.html">Liens</a></li>
        </ul>
      </div>
    {% endblock %}

    {% block sitephysuser_section %}
      <br />
      <div id="ho">Erreur - Eléments de physique en méthodes et langages objet</div>
      <br />
    {% endblock %}
    
    <br />

    <p></p><br /><br />
 
    {% block sitephysuser_body %}
      <br />
      <div id="ho">Erreur - Méthodes et langage objet pour la physique</div>
      <br /><br /><br />
      <a href="{{ path('sitephys_physmvc_home') }}">Retour à l'accueil du site</a>
    {% endblock %}

    <br />

    {% block javascripts %}
      {% javascripts 'bundles/sitephysphysmvc/js/jquery-3.1.1.min.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
      {% javascripts 'bundles/sitephysphysmvc/js/main.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
      {% javascripts 'bundles/sitephysphysmvc/js/bootstrap.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
    {% endblock %}

  </body>
  
</html>
