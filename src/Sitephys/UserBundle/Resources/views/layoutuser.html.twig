<!DOCTYPE html>
<html>

  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>
      {% block title %}
         Gestion des utilisateurs 
      {% endblock %}
    </title>
    {% block stylesheets %}
      {% stylesheets filter='cssrewrite' 'bundles/sitephysuser/css/styleuser.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
      {% endstylesheets %}
      {% stylesheets filter='cssrewrite' 'bundles/sitephysuser/css/mainuser.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
      {% endstylesheets %}
      {% stylesheets filter='cssrewrite' 'bundles/sitephysuser/css/bootstrap.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css">
      {% endstylesheets %}
    {% endblock %}
  </head>

  <body>
    {% block menu %}
      <div id="menu_nav">
        <ul class="menu cf">
          <li><a href="{{ path('sitephys_physmvc_home') }}">Accueil</a></li>
          <li><a href="{{ path('sitephys_physmvc_topic') }}">Thèmes</a></li>
          <li><a href="{{ path('sitephys_physmvc_symbolization') }}">Niveaux</a></li>
          <li><a href="{{ path('sitephys_physmvc_edition') }}">Edition</a></li>
          <li><a href="{{ asset('/login') }}"> {% if userconnect is defined %} {{ userconnect }} {% else %} {{ "Connexion" }} {% endif %}</a></li>
          <li><a href="{{ asset('../logout') }}">Déconnexion</a></li>
          <li><a href="{{ path('sitephys_user_account') }}">Compte</a></li>
          <li><a href="http://enseignant_postbac.nexgate.ch/index.html">Liens</a></li>
        </ul>
      </div>
    {% endblock %}

    <br />
    <h2 style = "color:#FA7; text-align:center; font-style:italic;">Méthodes et langage objet pour la physique</h2>
    <br />

    {% block sitephysuser_section %}
    {% endblock %}
    
    <br />

    {% block sitephysuser_image %}
    {% endblock %}

    <p></p><br /><br />
 
    {% block sitephysuser_body %}
    {% endblock %}

    <br />

    {% block javascripts %}
      {% javascripts 'bundles/sitephysuser/js/jquery-3.1.1.min.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
      {% javascripts 'bundles/sitephysuser/js/main.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
      {% javascripts 'bundles/sitephysuser/js/bootstrap.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
    {% endblock %}

  </body>
  
</html>